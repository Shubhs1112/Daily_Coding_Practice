var newArr = [];
var n;
function cumulativeSum(arr){
    for(let i=0; i<arr.length; i++){
         if(i==0){
             newArr.push(arr[i]);
         }
         else{
             n = arr[i]+arr[i-1];
             newArr.push(n);
             arr[i]= n;
         }
    }
    console.log(newArr);
}

cumulativeSum([1,2,3,4]);
cumulativeSum([1, 1, 1, 1, 1]);
cumulativeSum([1, 3, 5, 7, 9]);
